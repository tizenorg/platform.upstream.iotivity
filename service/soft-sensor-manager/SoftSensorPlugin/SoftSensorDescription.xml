        <value>1.0</value>
      </attribute>
      <attribute>
        <name>lifetime</name>
        <type>int</type>
        <value>60</value>
      </attribute>
    </attributes>
    <outputs>
      <output>
        <name>BMIresult</name>
        <type>int</type>
      </output>
    </outputs>
    <inputs>
     <input>Thing_WeightSensor</input>  
     <input>Thing_HeightSensor</input>
    </inputs>
